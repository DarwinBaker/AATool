                <flow_panel cell_height="52" direction="toptobottom" draw_mode="childrenonly" padding_top="32">
                    <panel height="38" draw_mode="childrenonly">
                        <picture texture="ab_example_red" width="38" height="38" margin_right="32" horizontal_align="left"/>
                        <textblock text="Left-click unplaced blocks to highlight them red.&#xA;(good for staying organized)" width="150" margin_left="48" padding_bottom="2" text_align="left" margin_right="32" horizontal_align="left"/>
                    </panel>
                    <panel height="38" draw_mode="childrenonly">
                        <picture texture="ab_example_gold" width="38" height="38" margin_right="32" horizontal_align="left"/>
                        <textblock text="Blocks will automatically light up gold once placed down anywhere in the world." width="150" margin_left="48" padding_bottom="2" text_align="left" margin_right="32" horizontal_align="left"/>
                    </panel>
                    <panel height="38" draw_mode="childrenonly">
